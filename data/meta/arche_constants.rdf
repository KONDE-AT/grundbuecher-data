        <acdh:isContinuedBy type="resource">$item/@next</acdh:isContinuedBy>
        <acdh:hasNonLinkedIdentifier type="literal">concat("Archivsignatur: ", $item//tei:idno[@type="archive"]/text())</acdh:hasNonLinkedIdentifier>
        <acdh:continues type="resource">$item/@pref</acdh:continues>
